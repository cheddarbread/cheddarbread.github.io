{
    "name": "cheddarbread_site",
    "version": "0.2.1",
    "description": "Delicious applications, baked to perfection. This is a site for all of my personal projects.",
    "scripts": {
        "build:css": "sheetloaf  \"sass/**/*.scss\" --dir css/ --base sass/ --style compressed --use autoprefixer,postcss-custom-properties --watch --no-source-map",
        "build:unicodestove:js": "esbuild unicodestove/scripts/index.js --outfile=unicodestove/scripts/index.bundle.js --bundle --watch",
        "build:unicodestove:css": "sheetloaf unicodestove/sass/style.scss --output unicodestove/css/style.css --style compressed --use autoprefixer,postcss-custom-properties --watch --no-source-map"
    },
    "author": "Benjamin Richardson",
    "dependencies": {
        "any-ascii": "^0.3.0",
        "autoprefixer": "^10.4.0",
        "esbuild": "^0.14.5",
        "postcss": "^8.4.5",
        "postcss-custom-properties": "^12.0.1",
        "sheetloaf": "^1.0.5"
    },
    "license": "ISC",
    "repository": "https://github.com/cheddarbread/cheddarbread.github.io"
}
